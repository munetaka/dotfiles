# Terminal Colorの設定
export CLICOLOR=1
export LSCOLORS=gxfxcxdxbxegedabagacad

# avr-gcc
export PATH="/usr/local/opt/avr-gcc@7/bin:$PATH"
export FZF_CTRL_T_COMMAND='rg --files --hidden --follow --glob "!.git/*"'
